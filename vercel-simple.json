{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.js"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/backend/server.js"  
    },
    {
      "source": "/health",
      "destination": "/backend/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/backend/server.js"
    }
  ]
}