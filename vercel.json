{
  "buildCommand": "echo 'Cache bust deployment - 2025-09-19T06:56:55Z'",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/events/*/checkin.js": {
      "maxDuration": 30
    },
    "api/events/*/checkin-v3.js": {
      "maxDuration": 30
    },
    "api/events/*/test-checkin.js": {
      "maxDuration": 30
    },
    "api/seed-database.js": {
      "maxDuration": 30
    },
    "api/check-events.js": {
      "maxDuration": 30
    },
    "api/events/[id]/qr.js": {
      "maxDuration": 30
    },
    "api/qr-checkin.js": {
      "maxDuration": 30
    },
    "api/events.js": {
      "maxDuration": 30
    },
    "api/event-qr.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
